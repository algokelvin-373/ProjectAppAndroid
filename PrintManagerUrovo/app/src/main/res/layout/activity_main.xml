<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_gravity="center"
    android:gravity="center"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btn_print"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/print"/>

    <Button
        android:id="@+id/btn_print_02"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/print_font_roboto_30"/>

    <Button
        android:id="@+id/btn_print_03"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/print_font_roboto_15"/>

    <Button
        android:id="@+id/btn_print_04"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/print_logo"/>

    <Button
        android:id="@+id/btn_print_05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/print_custome_font_size"/>

</LinearLayout>